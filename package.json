{
  "name": "@dmail/prettiest",
  "version": "3.0.0",
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmail/prettiest"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "index.js",
  "files": [
    "index.js",
    "src",
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-spread": "7.0.0",
    "@dmail/assert": "github:dmail/assert",
    "@dmail/project-structure": "github:dmail/project-structure",
    "@dmail/project-eslint-config": "github:dmail/project-eslint-config",
    "@dmail/project-prettier-config": "github:dmail/project-prettier-config",
    "babel-eslint": "11.0.0-beta.0",
    "prettier": "1.15.3",
    "codecov": "2.3.0",
    "nyc": "11.2.1",
    "rimraf": "2.6.2",
    "eslint": "5.1.0",
    "eslint-plugin-import": "2.6.0",
    "rollup": "0.66.2",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-babel": "4.0.3"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "node ./script/format.js",
    "compile": "node ./script/compile.js",
    "clean": "rimraf dist && rimraf coverage && rimraf .nyc_output",
    "dist": "npm run clean && npm run compile",
    "test": "npm run dist && node ./dist/index.test.js",
    "cover": "npm run dist && nyc node ./dist/index.test.js",
    "coverage-upload": "codecov",
    "prepublishOnly": "npm run dist",
    "reinstall": "npm run clean && rimraf node_modules && npm install"
  }
}
